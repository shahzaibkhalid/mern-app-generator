{
  "name": "create-mern-app",
  "version": "1.0.0",
  "description": "Most simplest way to start and build scalable MERN apps.",
  "main": "index.js",
  "scripts": {
    "client-dev": "webpack-dev-server --port 4000 --hot --inline",
    "server-dev": "opn http://localhost:8000 && nodemon ./app/server/public/server.js localhost 8000",
    "start": "node ./app/server/public/server.js",
    "build": "webpack -p",
    "heroku-postbuild": "npm install --only=dev && npm install && npm run build"
  },
  "engines": {
    "node": "8.5.0"
  },
  "author": "Shahzaib Khalid",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "fs-extra": "^4.0.2",
    "mongodb": "^2.2.33",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "replace": "^0.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.12.1",
    "opn-cli": "^3.1.0",
    "serve": "^6.4.0",
    "style-loader": "^0.19.0",
    "svg-react-loader": "^0.4.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
